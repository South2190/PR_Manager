<Window x:Class="PR_Manager.inputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        FontFamily="Meiryo"
        Title="inputBox" Height="172" Width="375" ResizeMode="NoResize" Closing="Window_Closeing">
    <Grid>
        <TextBlock Name="Mess" FontSize="12" HorizontalAlignment="Left" Height="35" Margin="30,15,0,0" Text="TextBlock" VerticalAlignment="Top" Width="300"/>
        <TextBox Name="TextBox" HorizontalAlignment="Left" Margin="30,55,0,0" VerticalAlignment="Top" Width="300" Height="20" TextChanged="Char_Judge" TabIndex="0"/>
        <Button Name="CancelButton" Content="キャンセル" HorizontalAlignment="Left" Margin="155,90,0,0" VerticalAlignment="Top" Width="80" Height="22" Click="Window_Close" TabIndex="1"/>
        <Button Name="OKButton" Content="OK" HorizontalAlignment="Left" Margin="249,90,0,0" VerticalAlignment="Top" Width="80" Height="22" Click="Return_Value" TabIndex="2"/>
    </Grid>
</Window>
